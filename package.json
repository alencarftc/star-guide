{
  "name": "fa-vite-react",
  "private": false,
  "version": "0.3.0",
  "license": "MIT",
  "author": {
    "name": "Felipe Alencar",
    "email": "me@fecasti.tech",
    "url": "https://fecasti.tech"
  },
  "description": "My default template for React based on Vite",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "test": "vitest --dir=./",
    "build": "tsc && vite build --base=./",
    "preview": "vite preview",
    "prepare": "husky install",
    "coverage": "vitest --coverage --dir=./",
    "lint": "tsc --noEmit && eslint src/**/*.ts{,x} --cache",
    "prettier:write": "prettier -u -w --ignore-path .gitignore \"*.{ts,tsx,css,html}\"",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@storybook/addon-a11y": "^6.5.16",
    "@storybook/addon-essentials": "^7.0.0",
    "@storybook/addon-interactions": "^7.0.0",
    "@storybook/addon-links": "^7.0.0",
    "@storybook/blocks": "^7.0.0",
    "@storybook/react": "^7.0.0",
    "@storybook/react-vite": "^7.0.0",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@testing-library/dom": "^10.1.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/axios": "^0.14.0",
    "@types/react": "^18.0.31",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-istanbul": "^0.29.8",
    "babel-loader": "^9.1.2",
    "core-js": "^3.29.1",
    "eslint": "^8.37.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react": "^1.1.7",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.5.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-vitest": "^0.0.57",
    "husky": "^8.0.0",
    "jest-dom": "^4.0.0",
    "jsdom": "^21.1.1",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.7",
    "prop-types": "^15.8.1",
    "sass": "^1.60.0",
    "standard-version": "^9.5.0",
    "storybook": "^7.0.0",
    "typescript": "^5.0.3",
    "typescript-plugin-css-modules": "^5.0.0",
    "vite": "^4.2.0",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^0.29.8"
  }
}
